<view class="feedback-detail" style="{{(viewColor)}}"><view class="hd"><view class="item"><view class="label">反馈类型</view><view class="txt">{{detail.category}}</view></view><view class="item"><view class="label">问题分类</view><view class="txt">{{detail.type}}</view></view><view class="item"><view class="label">姓名</view><view class="txt">{{detail.realname}}</view></view><view class="item"><view class="label">电话/邮箱</view><view class="txt">{{detail.contact}}</view></view><block wx:if="{{detail.reply}}"><view class="item"><view class="label">回复状态</view><view class="txt color">已回复</view></view></block></view><view class="content"><view class="con">{{detail.content}}</view><block wx:if="{{detail}}"><view class="img-box"><block wx:for="{{detail.images}}" wx:for-item="item" wx:for-index="index" wx:key="index"><image class="img-list" src="{{item}}" mode="scaleToFill" data-event-opts="{{[['tap',[['clickImg',['$0'],[[['detail.images','',index]]]]]]]}}" bindtap="__e"></image></block></view></block></view><block wx:if="{{detail.reply}}"><view class="content"><view class="reply"><view class="reply-left"><image src="{{domain+'/static/images/gly.png'}}" mode></image></view><view class="reply-right"><view class="clearfix reply-right-top"><view class="pull-left">管理员</view><view class="pull-right">{{detail.update_time}}</view></view><view>{{detail.reply}}</view></view></view></view></block></view>