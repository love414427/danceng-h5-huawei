.mask-box .bg{z-index:9;position:fixed;left:0;bottom:100rpx;width:100%;height:100%;background:rgba(0,0,0,.5)}.mask-box .mask-content{z-index:10;position:fixed;left:0;bottom:100rpx;width:100%;background-color:#f5f5f5;border-radius:16rpx 16rpx 0 0;-webkit-transform:translate3d(0,200%,0);transform:translate3d(0,200%,0);transition:all .3s cubic-bezier(.25,.5,.5,.9)}.mask-box .mask-content .title-bar{position:relative;text-align:center;padding:30rpx 0;margin-bottom:20rpx;font-size:32rpx;color:#282828}.mask-box .mask-content .title-bar .close{position:absolute;right:30rpx;top:50%;-webkit-transform:translateY(-50%);transform:translateY(-50%)}.mask-box .mask-content .title-bar .close .iconfont{color:#8a8a8a}.mask-box .mask-content .box{padding:0 30rpx 60rpx}.mask-box .mask-content .box .check-item{display:flex;align-items:center;justify-content:space-between;height:40rpx;margin-bottom:50rpx;font-size:28rpx}.mask-box .mask-content .box .check-item.total{font-weight:700;font-size:32rpx}.mask-box .mask-content .box .check-item.total .radio{color:var(--view-priceColor)}.animated{-webkit-animation-duration:.3s;animation-duration:.3s}