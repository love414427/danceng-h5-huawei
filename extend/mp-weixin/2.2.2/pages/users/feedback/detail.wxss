.feedback-detail .hd{margin:20rpx;padding:30rpx;border-radius:10rpx;background-color:#fff}.feedback-detail .hd .item{display:flex;justify-content:space-between;margin-bottom:30rpx;font-size:28rpx;color:#282828}.feedback-detail .hd .item:last-child{margin-bottom:0}.feedback-detail .hd .item .txt{flex:1;margin-left:50rpx;text-align:right;color:#868686}.feedback-detail .hd .item .color{color:var(--view-theme)}.feedback-detail .content{position:relative;margin:20rpx;padding:30rpx;border-radius:10rpx;line-height:1.5;background-color:#fff}.feedback-detail .img-box{display:flex;flex-wrap:wrap;justify-content:space-between}.feedback-detail .img-box .img-list{border-radius:10rpx;margin-top:20rpx;width:210rpx;height:210rpx}.feedback-detail .reply{display:flex}.feedback-detail .reply .reply-left{width:70rpx}.feedback-detail .reply .reply-left uni-image, .feedback-detail .reply .reply-left image{width:50rpx;height:50rpx;border-radius:50%}.feedback-detail .reply .reply-right{width:calc(100% - 70rpx);color:#282828}.feedback-detail .reply .reply-right .reply-right-top{color:#999;margin-bottom:14rpx}