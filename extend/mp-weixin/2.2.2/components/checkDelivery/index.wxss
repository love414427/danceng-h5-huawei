.mask-box .bg{z-index:30;position:fixed;left:0;bottom:0;width:100%;height:100%;background:rgba(0,0,0,.5)}.mask-box .mask-content{z-index:40;position:fixed;left:0;bottom:0;width:100%;background-color:#fff;border-radius:16rpx 16rpx 0 0;-webkit-transform:translate3d(0,100%,0);transform:translate3d(0,100%,0);transition:all .3s cubic-bezier(.25,.5,.5,.9)}.mask-box .mask-content .title-bar{position:relative;text-align:center;padding:30rpx 0;margin-bottom:20rpx;font-size:32rpx;color:#282828}.mask-box .mask-content .title-bar .close{position:absolute;right:30rpx;top:50%;-webkit-transform:translateY(-50%);transform:translateY(-50%)}.mask-box .mask-content .title-bar .close .iconfont{color:#8a8a8a}.mask-box .mask-content .box{padding:0 30rpx}.mask-box .mask-content .box .check-item{display:flex;align-items:center;justify-content:space-between;height:40rpx;margin-bottom:50rpx;font-size:28rpx}.mask-box .mask-content .box .check-item .iconfont{font-size:38rpx;color:#ccc}.mask-box .mask-content .box .check-item .iconfont.icon-xuanzhong1{color:var(--view-theme)}.mask-box .mask-content .foot{padding:20rpx 30rpx;padding-bottom:calc(20rpx+ constant(safe-area-inset-bottom));padding-bottom:calc(20rpx + env(safe-area-inset-bottom));border-top:1px solid #f5f5f5}.mask-box .mask-content .foot .btn{width:100%;height:70rpx;line-height:70rpx;text-align:center;border-radius:35rpx;color:#fff;font-size:30rpx;background:var(--view-theme)}.animated{-webkit-animation-duration:.3s;animation-duration:.3s}